                        <div class="card categorycard center small-box">
                            <h3>State Tenders</h3>
                            <img src="/Images/Category/state.png" alt="All Tenders State" />
                            <p>Click on any state from below list to get its latest tenders Information.</p>
                            <div class="lineseperatorOrange"></div>
                            <div class="pricing-area" id="stateList">
                                @Html.Partial("partialSpinnerLoadingView")
                            </div>
                            <a href="@ConfigurationManager.AppSettings["TenderByStateUrl"].ToString()" class="btn btn-block btn-lg btn-danger">
