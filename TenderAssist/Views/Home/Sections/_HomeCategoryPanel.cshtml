@using System.Configuration;
<style>
    .pricing-area {
        min-height: 170px;
    }

    .categorycard {
        min-height: 385px;
        /*height: 450px !important;*/
        background-color: #fff;
        padding-bottom: 5px;
    }

        .categorycard h3 {
            color: #fff;
            font-size: 22px;
            font-weight: bold;
            background-color: #3c8dbc;
            border-color: #367fa9;
            padding: 10px;
            border-top-left-radius: 5px;
            border-top-right-radius: 5px;
        }

        .categorycard p {
            font-size: 15px;
            margin: 5px 0px;
            color: #4b4b4b;
            /*line-height: 29px;*/
        }

        .categorycard img {
            margin: 0;
            box-shadow: 0 1px 9px rgba(43, 59, 93, .29);
            cursor: pointer;
            background: #fff;
            position: relative;
            -webkit-transition: all .3s ease-out;
            transition: all .3s ease-out;
            width: 100px !important;
            height: 80px;
            border-radius: 5px;
            padding: 2px;
        }

        /*.categorycard p, .categorycard  .pricing-area {
            
            padding:5px;
        }*/

    .center {
        text-align: center !important;
    }
</style>

<div id="pricing">
    <div class="text-center home-title">
        <h2 class="pricinghomeh2">
            Supported Links <small>Search Tender By</small>
        </h2>
    </div>
    <div class="padTB10 padLR10">
        <div class="row">

